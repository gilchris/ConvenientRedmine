{
    "name": "__MSG_extName__",
    "description": "__MSG_extDescription__",
    "version": "1.2.0",
    "manifest_version": 3,
    "author": "gilchris",
    "default_locale": "en",
    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
